
  <div class="panel panel-default">
    <div class="panel-heading">
      <%= list.escape('title') %>
    </div>
    <ol class="list-group">
      <% list.cards().each(function (card) { %>
        <li class="list-group-item" data-id="<%= card.id %>" data-list-id="<%= list.id %>" data-card-order="<%= card.escape('rank') %>">
          <%= card.escape('title') %>
        </li>
      <% }) %>
    </ol>
    <div class="panel-footer new-card" data-id="<%= list.id %>">
      <p class='text-center'><a>Add a card...</a></p>
    </div>
    <div class="panel-footer panel-danger delete-list" data-id="<%= list.id %>">
      <p class='text-center'><a>Delete List</a></p>
    </div>
  </div>
